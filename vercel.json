{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd web/dashboard && npm install && npm run build",
  "outputDirectory": "web/dashboard/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://localhost:8000/api/:path*"
    }
  ]
}
